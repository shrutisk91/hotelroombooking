<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="558.0" prefWidth="1000.0" stylesheets="@../../NewFile.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HotelController">
   <children>
      <ImageView fitHeight="558.0" fitWidth="1000.0" layoutX="-1.0" pickOnBounds="true">
         <image>
            <Image url="@../../../../Desktop/css/Light-Wood-Background-Wallpaper.jpg" />
         </image>
      </ImageView>
      <Button layoutX="212.0" layoutY="177.0" mnemonicParsing="false" onAction="#addhotelactivity" text="Add Hotel" textFill="#6b1414">
         <font>
            <Font name="System Bold Italic" size="19.0" />
         </font></Button>
      <Button layoutX="180.0" layoutY="272.0" mnemonicParsing="false" onAction="#updatehotelactivity" text="Update stars for a hotel" textFill="#6b1414">
         <font>
            <Font name="System Bold Italic" size="19.0" />
         </font></Button>
      <Button layoutX="637.0" layoutY="177.0" mnemonicParsing="false" onAction="#viewhotelactivity" text="View Hotels" textFill="#6b1414">
         <font>
            <Font name="System Bold Italic" size="19.0" />
         </font></Button>
      <Button layoutX="636.0" layoutY="272.0" mnemonicParsing="false" onAction="#deletehotelactivity" text="Delete hotel" textFill="#6b1414">
         <font>
            <Font name="System Bold Italic" size="19.0" />
         </font></Button>
      <Button layoutX="400.0" layoutY="388.0" mnemonicParsing="false" onAction="#goback" text="Go Back to Main Menu" textFill="#6b1414">
         <font>
            <Font name="System Bold Italic" size="19.0" />
         </font></Button>
   </children>
</AnchorPane>
