<AnchorPane prefHeight="558.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DeleteHotel">
   <children>
      <ImageView fitHeight="558.0" fitWidth="1000.0" pickOnBounds="true">
         <image>
            <Image url="@../../../../Desktop/css/Light-Wood-Background-Wallpaper.jpg" />
         </image>
      </ImageView>
      <ComboBox fx:id="hotelcombo" layoutX="405.0" layoutY="139.0" prefWidth="150.0" promptText="select hotel" />
      <Text fill="#682a2a" layoutX="263.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Update rating stars for a hotel">
         <font>
            <Font name="System Bold Italic" size="28.0" />
         </font></Text>
      <TextField fx:id="stars" layoutX="387.0" layoutY="240.0" promptText="rating stars" />
      <Button layoutX="447.0" layoutY="404.0" mnemonicParsing="false" onAction="#updatehotel" text="SUBMIT" />
      <Button layoutX="74.0" layoutY="488.0" mnemonicParsing="false" onAction="#goback" text="Go Back to Main Menu" />
   </children>
</AnchorPane>
